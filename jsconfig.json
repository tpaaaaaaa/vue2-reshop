{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    
  },
  "exclude":["node_modules","dist"],
  "vueCompilerOptions": {
    "targer":2.7,
    "experimentalDisableTemplateSupport": true,
    "extensions": [
      ".vue"
    ]
  }
}
